{
  "name": "flow-meter-pulse-multiplier-web-app",
  "version": "1.0.0",
  "description": "Web app for the ESP8266 Flow Meter Pulse Multipler",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-bulma": "sass --load-path=node_modules public/main.scss ../data/bulma.css",
    "start": "concurrently \"npm run build-bulma -- --watch\" \"pnpm run api\" \"webpack serve --mode development --open\"",
    "clean": "rimraf dist/"
  },
  "engines": {
    "node": ">=19.8.1",
    "pnpm": ">=8.15.4"
  },
  "keywords": [
    "flow meter",
    "pulse",
    "calibration",
    "scaling"
  ],
  "author": "Joshua Jenkins",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "babel-loader": "^9.1.3",
    "bulma": "^1.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^5.6.0",
    "json-server": "0.17.4",
    "mini-css-extract-plugin": "^2.8.1",
    "purgecss-webpack-plugin": "^6.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.74.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "mithril": "^2.2.2"
  }
}